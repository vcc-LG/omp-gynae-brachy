{% extends 'myapp/base.html' %}


{% block content %}
<style type="text/css">
textarea {
  width: 600px;
  height: 600px;
}
</style>

        <h1>Paste DVH data into box:</h1>
        <br />

        <form method="POST" class="post-form">{% csrf_token %}
            {{ form.as_p }}
            <button type="submit" id="submit" onClick="return empty()" class="save btn btn-default">Submit</button>
        </form>

        <script>
       function empty() {
          var x;
          x = document.getElementById("id_dump").value;
          if (x == "") {
              alert("Please paste DVH data in box");
              return false;
          };
      }
        </script>
{% endblock %}
